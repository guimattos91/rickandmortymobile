{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "assets/*": [
        "./src/assets/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "context/*": [
        "./src/context/*"
      ],
      "config/*": [
        "./src/config/*"
      ],
      "env/*": [
        "/src/env/*"
      ],
      "helpers/*": [
        "./src/helpers/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "routes/*": [
        "./src/routes/*"
      ],
      "screens/*": [
        "./src/screens/*"
      ],
      "services/*": [
        "./src/services/*"
      ],
      "theme/*": [
        "./src/theme/*"
      ],
      "types/*": [
        "./src/types/*"
      ]
    }
  }
}